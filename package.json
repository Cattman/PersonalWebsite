      }
    },
    "prod": {
      "deploy": {
        "path": "\\\\app02-dvl\\sites\\tftdvl\\production\\anodizing\\anodizingreceivingapp",
        "e2eUrl": "http://host/#/",
        "baseHref": "/production/anodizing/anodizingreceivingapp/"
      }
    }
  },
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "parallelshell \"ng serve\" \"npm run watch:custom-css\"",
    "htmlhint": "htmlhint --config .htmlhintrc --ignore **/global/** src/**/*.{htm,html}",
    "jshint": "jshint .",
    "lint": "ng lint --type-check",
    "lint:markdown": "markdownlint --config markdownlint.json README.md",
    "check": "npm run htmlhint && npm run lint && npm run lint:markdown && npm run jshint",
    "unit": "ng test --code-coverage --no-progress --browsers=ChromeHeadless --watch=false",
    "component": "echo component tests not implemented",
    "e2e:dev": "ng e2e --protractor-config=protractor-ci-dev.conf.js --dev-server-target=",
    "e2e:dev:headless": "ng e2e --protractor-config=protractor-ci-dev-headless.conf.js --dev-server-target=",
    "e2e:prod": "ng e2e --protractor-config=protractor-ci-prod.conf.js --dev-server-target=",
    "e2e:prod:headless": "ng e2e --protractor-config=protractor-ci-prod-headless.conf.js --dev-server-target=",
    "cleancss:custom-css": "cleancss -o src/assets/application/custom.min.css src/assets/application/custom.css",
    "watch:custom-css": "onchange \"src/assets/application/custom.css\" -v -- npm run cleancss:custom-css",
    "package": "npm run cleancss:custom-css",
    "webdriver:update": "webdriver-manager update",
    "deploy": "ng build ---configuration=dev --base-href \"%npm_package_config_dev_deploy_baseHref%\"",
    "deploy:prod": "ng build --prod ---configuration=production --base-href \"%npm_package_config_prod_deploy_baseHref%\"",
    "build": "npm run check && npm run package && npm run unit && npm run component && npm run deploy",
    "build:prod": "npm run check && npm run package && npm run unit && npm run component && npm run deploy:prod",
    "publish": "rimraf %npm_package_config_dev_deploy_path% && cpx \"dist/**\" %npm_package_config_dev_deploy_path%",
    "publish:prod": "rimraf %npm_package_config_prod_deploy_path% && cpx \"dist/**\" %npm_package_config_prod_deploy_path%"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Cattman/PersonalWebsite.git"
  },
